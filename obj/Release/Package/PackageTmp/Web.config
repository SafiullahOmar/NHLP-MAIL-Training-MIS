    </pages>
    <authentication mode="Forms">
      <forms loginUrl="login.aspx" defaultUrl="DashBoard.aspx" name="logfrm2" requireSSL="false" timeout="80" path="/" />
    </authentication>
    <roleManager cacheRolesInCookie="true" cookieName=".Tra" defaultProvider="AspNetSqlRoleProvider" enabled="true">
      <providers>
        <clear/>
        <add name="AspNetSqlRoleProvider" applicationName="Tra" type="System.Web.Security.SqlRoleProvider" connectionStringName="constr"/>
      </providers>
    </roleManager>
    <membership defaultProvider="AspNetSqlProvider">
      <providers>
        <clear/>
        <add connectionStringName="constr" applicationName="Tra" name="AspNetSqlProvider" type="System.Web.Security.SqlMembershipProvider" enablePasswordRetrieval="false" enablePasswordReset="true" requiresQuestionAndAnswer="false" requiresUniqueEmail="true" maxInvalidPasswordAttempts="60" minRequiredPasswordLength="3" minRequiredNonalphanumericCharacters="0" passwordAttemptWindow="60"/>
      </providers>
    </membership>
    <profile defaultProvider="AspNetSqlProfileProvider" enabled="true">
      <providers>
        <clear/>
        <add name="AspNetSqlProfileProvider" type="System.Web.Profile.SqlProfileProvider" connectionStringName="constr" applicationName="Tra"/>
      </providers>
      <properties>
        <add name="FullName" type="String"/>
        <add name="Age" type="String"/>
        <add name="Province" type="String"/>
        <add name="District" type="String"/>
        <add name="Designation" type="String"/>
      </properties>
    </profile>
  </system.web>
  <!-- 
